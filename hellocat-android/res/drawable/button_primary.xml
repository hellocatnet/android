<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape android:shape="rectangle">
            <corners android:radius="3dip" />

            <stroke android:width="1dip" android:color="#233555" />

            <gradient android:angle="-90" android:endColor="#314A77" android:startColor="#314A77" />
        </shape></item>
    <item android:state_focused="true"><shape android:shape="rectangle">
            <corners android:radius="3dip" />

            <stroke android:width="1dip" android:color="#4B71B6" />

            <solid android:color="#4B71B6" />
        </shape></item>
    <item><shape android:shape="rectangle">
            <corners android:radius="3dip" />

            <stroke android:width="1dip" android:color="#293E65" />

            <gradient android:angle="-90" android:endColor="#2F4979" android:startColor="#6484C0" />
        </shape></item>

</selector>